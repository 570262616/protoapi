import Vue from 'vue';
import Vuetify from 'vuetify';
import {{.ClassName}} from './{{.ClassName}}';

let tmpl = require('./index.html')
Vue.use(Vuetify)

const app = Vue.extend({
    template: tmpl,
    data: () =>({
        service: new {{.ClassName}}(),
    }),
    methods:{
        {{range .Functions}}
        {{.Name}}(){
            this.service.{{.Name}}()
        },
        {{end}}
    }
})

/**
 * load to index.html by id="app"
 */
new app().$mount('#app')
  