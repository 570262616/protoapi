<?php

namespace app\modules\api;

use app\modules\api\models;

class RequestHandler extends handlers\RequestHandler{
    function demo($req) {
        $bizError = new models\BizError();
        $bizError->set_message("This is a biz error.");
        throw $bizError;

        $res = new models\DemoResponse();
        $res->set_reply($req->get_text());
        return $res;
    }
}
